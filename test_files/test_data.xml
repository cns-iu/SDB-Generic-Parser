    <!--{Test2} -->
    <!--Almost the same as {Test1}. The property4 tag is replaced with property4_mod1, which triggers a new insertion with
    each counted item instead of keeping them in the same statement.-->
    <REC>
        <UID>test:000002</UID>
        <summary>
            <property1 property1_attrib1="attrib1_1_value" />
            <property2 property2_attrib1="attrib2_1_value" property2_attrib2="attrib2_2_value">Property Two Text Element</property2>
            <property3>Property Three Text Element</property3>
            <property4_mod1>
                <property_count property4_1_attrib1="property_1_value">Property Four (1) Text Element (Count: 1)</property_count>
                <property_count property4_1_attrib1="property_2_value">Property Four (1) Text Element (Count: 2)</property_count>
            </property4_mod1>
        </summary>
    </REC>
